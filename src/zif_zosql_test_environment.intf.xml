  <asx:values>
   <VSEOINTERF>
    <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>Test data environment for use in fake database operations</DESCRIPT>
    <EXPOSURE>2</EXPOSURE>
    <STATE>1</STATE>
    <UNICODE>X</UNICODE>
   </VSEOINTERF>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>CLEAR_ALL</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Clears all data in store</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>CLEAR_DOUBLES</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Clears all data in test doubles (equivalent to CLEAR_ALL)</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>CLEAR_ONE_TABLE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Clears data of one table in virtual store</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>DELETE_TEST_DATA_FROM_ITAB</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Deletes certain table records in test data by keys in itab</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>GET_DATA_OF_TABLE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns data of one virtual database table</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>GET_DATA_OF_TABLE_AS_REF</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns data of one virtual database table as ref to data</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>GET_DB_LAYER_FOR_UNIT_TESTS</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Create instance of zif_zosql_db_layer for use in unit tests</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CLSNAME>ZIF_ZOSQL_TEST_ENVIRONMENT</CLSNAME>
     <CMPNAME>INSERT_TEST_DATA</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Insert test data to fake store of table from internal table</DESCRIPT>
    </SEOCOMPOTX>
   </DESCRIPTIONS>
  </asx:values>
